  <link name="rov_base_link">
    <visual>
      <geometry>
	      <mesh filename="meshes/reach5mini/visual/BlueRov2.dae" />
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="meshes/reach5mini/visual/BlueRov2.dae" />
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
  </link>

  <joint name="rov_base_joint" type="fixed">
    <parent link="base_link"/>
    <child link="rov_base_link"/>
    <!-- the relative pose between rov and arm -->
    <origin xyz="-0.2 0.15 -0.3" rpy="3.14 0 0"/>
  </joint>

  <link name="rov_payload_link">
    <visual>
      <geometry>
	      <mesh filename="meshes/reach5mini/visual/BROV-PAYLOAD-SKID-R1.dae" scale="0.001 0.001 0.001"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="meshes/reach5mini/visual/BROV-PAYLOAD-SKID-R1.dae" scale="0.001 0.001 0.001"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
  </link>

  <joint name="rov_payload_joint" type="fixed">
    <parent link="rov_base_link"/>
    <child link="rov_payload_link"/>
    <!-- the relative pose between rov and payload -->
    <origin xyz="-0.25 -0.165 -0.325" rpy="1.57 0 1.57"/>
  </joint>

  <link name="base_link">
    <visual>
      <geometry>
	<mesh filename="meshes/reach5mini/visual/Base.obj" />
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.150" radius="0.020"/>
      </geometry>
      <origin rpy="0 1.57075 0" xyz="-0.075 0 0"/>
    </collision>
    <!-- <inertial>
      <mass value="0.341"/>
      <origin xyz="-0.075 -0.006 -0.003"/>
