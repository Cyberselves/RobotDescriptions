<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
	<xacro:macro name="elephantbot_mycobot_pro_320" params="base_link end_link">

        <joint name="cobot_base_joint" type="fixed">
            <parent link="${base_link}"/>
            <child link="cobot_base"/>
            <origin rpy="0 0 1.57080" xyz="0 0 0"/>
            <axis xyz="0 0 1"/>
        </joint>

        <link name="cobot_base">
            <visual>
                <geometry>
                    <mesh filename="meshes/elephantbot/base.dae" scale="0.001 0.001 0.001"/>
                </geometry>
                <origin xyz="0.0 0 0 " rpy="0 0 1.5708"/>
            </visual>
            <collision>
                <geometry>
                    <mesh filename="meshes/elephantbot/base.dae" scale="0.001 0.001 0.001"/>
                </geometry>
                <origin xyz="0.0 0 0" rpy="0 0 1.5708"/>
            </collision>
        </link>

        <joint name="cobot_joint2_to_joint1" type="revolute">
            <axis xyz="0 0 1"/>
            <limit effort="1000.0" lower="-2.96" upper="2.96" velocity="0"/>
            <parent link="cobot_base"/>
            <child link="cobot_link1"/>
            <origin xyz="0 0 0.173" rpy="0 0 0"/>
        </joint>

        <link name="cobot_link1">
            <visual>
                <geometry>
                    <mesh filename="meshes/elephantbot/link1.dae" scale="0.001 0.001 0.001"/>
                </geometry>
                <origin xyz="0.0 0.0 -0.078" rpy="0 0 0"/>
            </visual>
            <collision>
                <geometry>
                    <mesh filename="meshes/elephantbot/link1.dae" scale="0.001 0.001 0.001"/>
                </geometry>
                <origin xyz="0.0 0.0 -0.078 " rpy="0 0 0"/>
            </collision>
        </link>

        <joint name="cobot_joint3_to_joint2" type="revolute">
            <axis xyz="0 0 1"/>
            <limit effort="1000.0" lower="-2.79" upper="2.79" velocity="0"/>
            <parent link="cobot_link1"/>
            <child link="cobot_link2"/>
            <origin xyz="-0.086 0 0" rpy="0 1.57080 3.14"/>
        </joint>

        <link name="cobot_link2">
            <visual>
                <geometry>
                    <mesh filename="meshes/elephantbot/link2.dae" scale="0.001 0.001 0.001"/>
                </geometry>
                <origin xyz="0.0 0.0 -0.04" rpy="3.1416 1.5708 0"/>
