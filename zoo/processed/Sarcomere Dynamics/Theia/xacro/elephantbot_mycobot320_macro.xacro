        </link>

        <joint name="tool_joint" type="revolute">
            <parent link="cobot_link6"/>
            <child link="${end_link}"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </joint>
    </xacro:macro>
</robot>
