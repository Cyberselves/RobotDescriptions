                </geometry>
                <origin xyz="0.0 0.0 0.04" rpy="1.5708 1.5708 -1.5708"/>
            </collision>
        </link>

        <joint name="cobot_joint5_to_joint4" type="revolute">
            <axis xyz="0 0 1"/>
            <limit effort="1000.0" lower="-2.79" upper="2.79" velocity="0"/>
            <parent link="cobot_link3"/>
            <child link="cobot_link4"/>
            <origin xyz="-0.1195 0 0.082" rpy="0 0 1.57080"/>
        </joint>

        <link name="cobot_link4">
