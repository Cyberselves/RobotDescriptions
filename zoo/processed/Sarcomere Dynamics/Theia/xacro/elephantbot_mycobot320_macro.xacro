                <geometry>
                    <mesh filename="meshes/elephantbot/link5.dae" scale="0.001 0.001 0.001"/>
                </geometry>
                <origin xyz="0.0015 -0.05 0" rpy="-1.5708 0 0"/>
            </visual>
            <collision>
                <geometry>
                    <mesh filename="meshes/elephantbot/link5.dae" scale="0.001 0.001 0.001"/>
                </geometry>
                <origin xyz="0.0015 -0.05 0" rpy="-1.5708 0 0"/>
            </collision>
        </link>

        <joint name="cobot_joint6output_to_joint6" type="revolute">
            <axis xyz="1 0 0"/>
